{
  "name": "APT2",
  "version": "1.0.0",
  "main": "web/index.js",
  "license": "UNLICENSED",
  "scripts": {
    "dev:storefront": "cd apt2/storefront && yarn dev",
    "dev_admin:be": "cd apt2/admin && yarn dev",
    "dev_admin:fe": "cd apt2/admin/frontend && yarn dev",
    "typecheck": "tsc --noEmit",
    "prettier:code": "prettier \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:other": "prettier \"**/*.{md,css,scss,yaml,yml}\"",
    "format:code": "npm run prettier:code -- --write",
    "format:other": "npm run prettier:other -- --write",
    "format": "npm-run-all -p format:*",
    "remove_depend:root": "rm -rf node_modules/ yarn.lock package-lock.json",
    "remove_depend:backend": "cd apt2/admin && rm -rf node_modules/ yarn.lock package-lock.json",
    "remove_depend:admin_frontend": "cd apt2/admin/frontend && rm -rf node_modules/ yarn.lock package-lock.json",
    "remove_depend:storefront": "cd apt2/storefront/ && rm -rf node_modules/ yarn.lock package-lock.json",
    "remove_depend": "npm-run-all -p remove_depend:*",
    "install_depend": "yarn && cd apt2/admin && yarn && cd frontend/ && yarn && cd ../../ && cd storefront && yarn && cd ../../"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "dotenv": "^16.0.3",
    "gulp": "^4.0.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^9.1.4",
    "postcss-scss": "^4.0.9",
    "prettier": "^2.7.1",
    "typescript": "^5.2.2"
  },
  "author": "AbelTruong",
  "engines": {
    "npm": ">=8.0.0",
    "node": "^=18.x.x"
  }
}
